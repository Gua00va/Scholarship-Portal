// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Scholarship {
  name String @unique
  description String
  createdAt DateTime @default(now())
  startDate DateTime
  endDate DateTime
  link String
  isActive Boolean
}

model User {
  email String @unique
  password String @unique
}

